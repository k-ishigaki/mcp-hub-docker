{
  "$schema": "https://raw.githubusercontent.com/himorishige/hatago-mcp-hub/main/schemas/config.schema.json",
  "version": 1,
  "timeouts": {
    "requestMs": 120000,
    "keepAliveMs": 15000
  },
  "mcpServers": {
    "deepwiki": {
      "url": "https://mcp.deepwiki.com/mcp",
      "timeouts": {
        "requestMs": 60000
      },
      "tags": ["dev"]
    },
    "google-calendar": {
      "command": "npx",
      "args": ["-y", "@cocal/google-calendar-mcp@latest"],
      "env": {
        "GOOGLE_OAUTH_CREDENTIALS": "/app/gcp-oauth.keys.json"
      },
      "timeouts": {
        "requestMs": 60000
      },
      "tags": ["dev"]
    }
  }
}
